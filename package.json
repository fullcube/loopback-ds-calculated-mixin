{
  "name": "loopback-ds-calculated-mixin",
  "version": "0.0.0-development",
  "description": "A mixin for the LoopBack framework that adds calculated properties to a model.",
  "keywords": [
    "loopback",
    "strongloop",
    "mixin"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fullcube/loopback-ds-calculated-mixin.git"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "NODE_ENV=test nyc --reporter=lcov --reporter=text --reporter=text-summary mocha test/*test.js",
    "test:watch": "npm run test -- -w",
    "pretest": "npm run lint",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "main": "lib/index.js",
  "dependencies": {
    "debug": "^2.6.4",
    "depd": "^1.1.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "bluebird": "latest",
    "chai": "latest",
    "chai-datetime": "^1.4.1",
    "condition-circle": "^1.5.0",
    "dirty-chai": "^1.2.2",
    "eslint": "^2.11.1",
    "eslint-config-fullcube": "^1.0.46",
    "loopback": "^3.6.0",
    "loopback-datasource-juggler": "^3.5.0",
    "mocha": "latest",
    "mocha-sinon": "latest",
    "nyc": "^10.2.0",
    "semantic-release": "^6.3.2",
    "sinon": "latest",
    "sinon-chai": "latest"
  },
  "release": {
    "verifyConditions": "condition-circle"
  }
